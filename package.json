{
  "name": "rxjs-way",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_HOST=http://localhost:3000 next dev",
    "start": "next start",
    "lint": "next lint",
    "prettier": "npx prettier . --write",
    "build": "cross-env NEXT_PUBLIC_HOST=https://rxjs-way.vercel.app next build",
