    "dompurify": "^3.0.3",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "github-markdown-css": "^5.2.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.8.0",
    "jsdom": "^22.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-highlightjs": "^4.0.1",
    "next": "13.4.17",
